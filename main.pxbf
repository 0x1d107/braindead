$include('braindead.pyex')
$include('stack.pyex')
$alloc('n','e','t')
$stack_alloc('c','d')
$read('n')
$set('e',char('0'))
$loopdo('e')
    $dec('n')
    $dec('e')
$loopdone()
$set('c',0)
$loopdo('n')
    $read('d')
    $stack_swp()
    $inc('c')
    $dec('n')
$loopdone()
$loopdo('c')
    $dec('c')
    $stack_swp()
    $write('d')
    $set('d',0)
$loopdone()