$include('braindead.pyex')
$alloc('a','b','c','w','t','x') 
$set('a',0)
$set('b',1)
$set('x',7)
$loopdo('x')
    $set('w',0)
    $move('a','t','w')
    $move('t','a')
    $add('w',char('0'))
    $write('w')
    $set('c',0)
    $move('a','c')
    $move('b','a','c')
    $move('c','b')    
    $dec('x')
$loopdone()
